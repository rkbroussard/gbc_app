<%= form_for(@bulletin) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="well">
  	<%= f.label :name %>
  	<%= f.text_field :name %>
  	<%= f.label :attachment %>
  	<%= f.file_field :attachment %>
  <%= f.submit "Save", class: "btn btn-large btn-primary" %>
  </div>
<% end %>